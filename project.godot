; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "World_dao",
"language": "GDScript",
"path": "res://Script/World.gd"
} ]
_global_script_class_icons={
"World_dao": "res://Image/dao/world.png"
}

[application]

config/name="Phantomworld"
config/description="the world begins"
run/main_scene="res://scences/os/os_load.tscn"
boot_splash/image="res://Image/phantom_os_splash.png"
boot_splash/use_filter=false
config/icon="res://UI/logo.png"
config/macos_native_icon="res://UI/logo.icns"

[autoload]

Global="*res://Scripts/global.gd"

[display]

window/size/width=2560
window/size/height=1440
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://Image/os/mm.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "btree", "godot-behavior-tree-plugin" )

[gui]

theme/use_hidpi=true

