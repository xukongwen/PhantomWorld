; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "World_dao",
"language": "GDScript",
"path": "res://Script/World.gd"
} ]
_global_script_class_icons={
"World_dao": "res://Image/dao/world.png"
}

[application]

config/name="Phantomworld"
config/description="the world begins"
run/main_scene="res://scences/os/os_load.tscn"
boot_splash/image="res://Image/phantom_os_splash.png"
boot_splash/use_filter=false
config/icon="res://UI/logo.png"
config/macos_native_icon="res://UI/logo.icns"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://Image/os/mm.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/use_hidpi=true

[input]

pause={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":80,"unicode":0,"echo":false,"script":null)
 ]
}

[rendering]

environment/default_clear_color=Color( 0.847059, 0.847059, 0.847059, 1 )
environment/default_environment="res://default_env.tres"
